                <td>
                    @Html.DisplayFor(modelItem => item.DiagTool)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SortOrder)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreateUserId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreateDt)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UpdateUserId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UpdateDt)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CompSeqNavigation.CompName)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.GroupSeq">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.GroupSeq">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.GroupSeq">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
